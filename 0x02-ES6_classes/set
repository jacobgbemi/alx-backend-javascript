set number
set pastetoggle=<F11>
set tabstop=4
set softtabstop=4
set shiftwidth=4
set smartindent
set autoindent
syntax enable
set expandtab
let g:syntastic_javascript_checkers=['standard']
let g:syntastic_javascript_standard_exec = 'semistandard'
autocmd bufwritepost *.js silent !semistandard % --fix
set autoread
